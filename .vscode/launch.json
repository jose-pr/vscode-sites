        "makemigrations",
        "vscode_marketplace"
      ],
      "django": true,
      "justMyCode": true
    },
    {
      "name": "Python: Import Data",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}\\src\\manage.py",
      "cwd": "${workspaceFolder}\\src",
